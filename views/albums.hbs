<div class="row row-cols-1 row-cols-md-3 g-4">
    {{#each allAlbums}}
    <div class="col">
        <div class="card h-100">
            <img src="{{this.images.[0].url}}" class="card-img-top" alt="image">
            <div class="card-body">
                <h5 class="card-title">{{this.name}}</h5>
                <button class="btn btn-danger"><a href="/album/{{this.id}}/tracks">Listen Tracks</a></button>
            </div>
        </div>
    </div>
    {{/each}}
</div>